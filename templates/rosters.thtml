{% extends "base.thtml" %}

{% block title %}Standard Errors Rosters{% endblock %}

{% block styles %}
    <link href="css/rosters.css" rel="stylesheet">
{% endblock %}

{% block content %}

<div id="content">

    <div class="container">

        <div class="row">
            <div id="legend" class="col-xs-12 text-right">
                <ul class="legend-list">
                    <li>★ - captain</li>
                    <li>↲ - non-RTI</li>
                </ul>
            </div>
        </div>

        <div class="row">
            <div id="rosters" class="col-xs-12">
            </div>
        </div>

    </div>

</div>

{% endblock %}
        
{% block scripts %}
    <script src="vendor/queue-async-1.0.7/queue.min.js"></script>
    <script src="js/rosters.js"></script>
{% endblock %}
